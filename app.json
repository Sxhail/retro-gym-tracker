{
  "expo": {
    "name": "Retro Gym Tracker",
    "slug": "retro-gym-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "retro-gym-tracker",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/2955f480-ac00-4dd4-be3e-35c023cc436b"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "bundleIdentifier": "com.sxhail.retrogymtracker",
      "supportsTablet": true,
      "requireFullScreen": false,
      "icon": "./app/assets/icon.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "audio",
          "fetch"
        ]
      }
    },
    "android": {
      "package": "com.sxhail.retrogymtracker",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "expo-asset",
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./app/assets/icon.png",
          "color": "#00FFC2"
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*",
      "app/assets/**/*"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2955f480-ac00-4dd4-be3e-35c023cc436b"
      }
    }
  }
}
